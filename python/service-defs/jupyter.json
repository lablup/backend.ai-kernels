{
  "prestart": [
    {
      "action": "write_tempfile",
      "args": {
        "body": [
          "c.NotebookApp.allow_root = True\n",
          "c.NotebookApp.ip = \"0.0.0.0\"",
          "c.NotebookApp.port = {ports[0]}",
          "c.NotebookApp.token = \"\"",
          "c.FileContentsManager.delete_to_trash = False"
        ]
      },
      "ref": "jupyter_cfg"
    }
  ],
  "command": [
    "{runtime_path}", "-m", "notebook", "--no-browser", "--config", "{jupyter_cfg}"
  ]
}
